<Window x:Class="Presentation_Clinician.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Presentation_Clinician"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Loaded="Window_Loaded">

    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Height="42">
            <Button x:Name="BtnStart" Content="Startside"  Width="80" Height="40" Margin="10,0,0,0" Click="BtnStart_Click"/>
            <Button x:Name="BtnPatient" Content="Patient" Width="80" Height="40" Margin="10,0,0,0" Click="BtnPatient_Click"/>
            <Button x:Name="BtnHearingAid" Content="Høreapparat" Width="80" Height="40" Margin="10,0,0,0" Click="BtnHearingAid_Click" />
            <Button x:Name="BtnProces" Content="Proces"  Width="80" Height="40" Margin="10,0,0,0" Click="BtnProces_Click" />

        </StackPanel>
        <Frame x:Name="Main" Margin="0,50,0,-1" NavigationUIVisibility="Hidden"/>

    </Grid>
</Window>
