<Page x:Class="Presentation_Clinician.ManageHAPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Presentation_Clinician"
      mc:Ignorable="d" 
      d:DesignHeight="400" d:DesignWidth="800"
      Title="ManageHAPage" Background="#FFF9F5F5">

    <Grid>
        <Button x:Name="BtnUpdate" Content="Opdater" HorizontalAlignment="Left" Margin="352,274,0,0" VerticalAlignment="Top" Height="31" Width="75" Click="BtnUpdate_Click" RenderTransformOrigin="2.187,0.516"/>
