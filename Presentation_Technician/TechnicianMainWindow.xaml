        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Presentation_Technician"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame x:Name="Main" Margin="0,35,0,0"/>

        <StackPanel Orientation="Horizontal" Height ="35" VerticalAlignment="Top">
            <Button x:Name="ManageHAB" Content="Manage HA" Margin="10,0,0,0" Click="ManageHAB_Click"></Button>
        </StackPanel>
    </Grid>

</Window>
